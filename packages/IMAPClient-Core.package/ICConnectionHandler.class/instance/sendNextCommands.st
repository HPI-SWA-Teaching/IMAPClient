connection
sendNextCommands    | hash conOK |    hash := self accountInfo hash.  conOK := true.    [conOK and: (self storedCommands isEmptyFor: hash) not] whileTrue: [    [self sendDirectCommand: (self storedCommands getNextCommandFor: hash).    self storedCommands popNextCommandFor: hash]   on: Error do: [conOK := false]]